	<queryString>
		<![CDATA[select CONCAT(u.nombres," ",u.apellidos) as nom_alumno,
       g.grupo,
       c.descripcion,
       p.pregunta,
       r.id_respuesta,
       r.respuesta,
       r.estado,
       ca.id_c_alumno,
       COALESCE(ra.id_respuesta, ra.id_respuesta, 0)  as id_respuesta2,
      (select rc.respuesta from respuestas_alumno r INNER JOIN respuestas_cuestionario rc
       ON r.id_respuesta = rc.id_respuesta WHERE r.id_pregunta = p.id_pregunta and r.id_c_alumno = ca.id_c_alumno ) as r_respuesta_alumno,
      (select id_respuesta from respuestas_alumno WHERE  id_pregunta = p.id_pregunta and id_c_alumno = ca.id_c_alumno ) as parametro,
      (select CASE WHEN (estado = 1) THEN 'Correcto' ELSE 'Incorrecto' END as cal from respuestas_cuestionario WHERE  id_respuesta = parametro) as calificacion
FROM c_cuestionario c
INNER JOIN  preguntas_cuestionario p on c.id_cuestionario = p.id_cuestionario
INNER JOIN respuestas_cuestionario r ON r.id_fk = p.id
INNER JOIN c_cuestionario_alumno ca ON ca.id_cuestionario = c.id_cuestionario AND ca.id_user=$P{alumno}
LEFT JOIN respuestas_alumno ra ON  ra.id_c_alumno = ca.id_c_alumno and ra.id_respuesta = r.id_respuesta
LEFT JOIN usuarios u  ON u.id_user = ca.id_user
INNER JOIN grupo g ON g.id_grupo = u.id_grupo
WHERE c.id_cuestionario = $P{idCuestionario}
ORDER BY  p.pregunta, r.id_respuesta]]>
	</queryString>
